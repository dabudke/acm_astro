---
import Layout from "$layouts/Layout.astro";
import { ArrowLeftOutlined } from "@ant-design/icons";
---

<Layout title="Not Found">
  <main class="my-8 ml-12">
    <h1 class="text-2xl">404 - Not Found</h1>
    <p class="mt-2">The link you clicked is either outdated, or incorrect.</p>
    <a
      href="/"
      class="text-display uppercase flex mt-2 gap-2"
      ><ArrowLeftOutlined /> Go to Homepage</a
    >
  </main>
</Layout>
